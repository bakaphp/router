{
    "name": "baka/router",
    "description": "A tool to create multiple Phalcon Collections with easy",
    "keywords": [
        "baka",
        "mctekk",
        "router",
        "phalcon"
    ],
    "homepage": "https://bakaphp.github.io/router",
    "license": "MIT",
    "authors": [
        {
            "name": "Esteban De la Rosa",
            "email": "gabbanaesteban@gmail.com",
            "homepage": "https://estebandelarosa.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-phalcon": ">=3.4",
        "phalcon/incubator": "^3.4",
        "baka/support": "^1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.2",
        "symfony/var-dumper": "^4.3@dev"
    },
    "autoload": {
        "psr-4": {
            "Baka\\Router\\": "src/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}